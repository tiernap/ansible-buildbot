{
  "slaves": [
{% for host in groups['buildbot-slave'] %}
{% if not loop.last %}
      { "os": "precise64", "name": "{{host}}" },
{% else %}
      { "os": "precise64", "name": "{{host}}" }
{% endif %}
{% endfor %}
    ],
  "projects": [
{% for repo, branches in repositories|dictsort %}
    {
      "name": "{{repo}}",
      "repourl": "{{repo_prefix}}/{{repo}}",
      "category": "active",
      "builders": [
{% for branch in branches %}
{% if not loop.last %}
          { "os": "precise64", "branch": "{{branch}}" },
{% else %}
          { "os": "precise64", "branch": "{{branch}}" }
{% endif %}
{% endfor %}
        ]
{% if not loop.last %}
    },
{% else %}
    }
{% endif %}
{% endfor %}
  ]
}
